<mvc:View xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core" controllerName="auths.controller.Detail">
    <Page id="detailPage" title="{RoleName}" showNavButton="false" navButtonPress="onNavBack">
        <content>
            <VBox id="vbContent" class="sapUiTinyMargin" />
        </content>
        <footer>
            <OverflowToolbar id="_IDGenOverflowToolbar1">
                <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                <Button id="_IDGenButton1" type="Emphasized" text="{i18n>Save}" press="onSaveData" />
                <Button id="_IDGenButton2" type="Default" text="{i18n>Cancel}" press="onNavBack" />
            </OverflowToolbar>
        </footer>
    </Page>
</mvc:View>
